{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level}][{Properties}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/OSLPrometheus.txt",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day",
          "retainedFileCountLimit": 10,
          "fileSizeLimitBytes": 104857600
        }
      }
    ]
  },
  "Settings": {
    "ConnectionStringDatabase": "Server=TUO_SERVER;Database=TUO_DATABASE;User Id=UTENTE;Password=PASSWORD;",
    "CsvFolder": "C:\\CartellaCSV",
    "ArchiveFolder": "C:\\ArchivioCSV"
  }
 }