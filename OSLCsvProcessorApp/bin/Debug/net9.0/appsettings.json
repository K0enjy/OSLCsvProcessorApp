{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level}][{Properties}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/OSLPrometheus.txt",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day",
          "retainedFileCountLimit": 10,
          "fileSizeLimitBytes": 104857600
        }
      }
    ]
  },
  "Settings": {
    "ConnectionStringDatabase": "Server=DEV-LORENZO;Database=GP90_BRUNINI;User Id=sa;Password=;TrustServerCertificate=true",
    "CsvFolder": "C:\\LAVORO\\Repository GitHub\\Lavoro-OSL\\File_Clienti\\BRUNINI\\{SVILUPPO}\\File",
    "ArchiveFolder": "C:\\LAVORO\\Repository GitHub\\Lavoro-OSL\\File_Clienti\\BRUNINI\\{SVILUPPO}\\File\\Elaborato"
  }
 }